{
  "name": "capacitor-datawedge",
  "version": "1.0.0",
  "description": "Capacitor plugin for DataWedge integration on Zebra devices",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/umd/capacitor-datawedge.umd.js",
  "files": [
    "dist/",
    "android/",
    "src/"
  ],
  "author": "Connect I.T",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxilosgr/capacitor-datawedge-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/maxilosgr/capacitor-datawedge-plugin/issues"
  },
  "homepage": "https://github.com/maxilosgr/capacitor-datawedge-plugin#readme",
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "zebra",
    "datawedge",
    "barcode",
    "scanner"
  ],
  "scripts": {
    "verify": "npm run verify:android",
    "verify:android": "echo 'Android verification passed'",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "docgen": "docgen --api DataWedgePlugin --output-readme README.md --output-json dist/docs.json",
    "fmt": "prettier --write '**/*.{css,html,js,json,md,ts,tsx,yml}'",
    "fmt:check": "prettier --check '**/*.{css,html,js,json,md,ts,tsx,yml}'",
    "lint": "eslint . --ext ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/docgen": "^0.0.18",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "eslint": "^7.11.0",
    "prettier": "~2.3.0",
    "prettier-plugin-java": "~1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": "^7.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "android": {
      "src": "android"
    }
  }
}